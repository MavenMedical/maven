if [ 0 ]; do
sudo useradd tom
sudo su tom
# begin as tom
cd
mkdir -m 700 .ssh
cd .ssh
echo ssh-rsa  id_rsa > authorized_keys
chmod 600 authorized_keys
exit
# end as tom

sudo useradd yuki
