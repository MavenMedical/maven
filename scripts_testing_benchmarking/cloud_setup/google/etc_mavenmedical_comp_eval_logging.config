[loggers]
keys=root,compeval,fhirDB,allscriptsServer

[handlers]
keys=tmpHandler,compEvalHandler,fhirDBHandler,allscriptsServerHandler

[formatters]
keys=verboseFormatter

[logger_root]
level=DEBUG
handlers=tmpHandler

[logger_compeval]
level=DEBUG
handlers=compEvalHandler
propagate=1
qualname=app.backend.evaluators.composition_evaluator

[logger_fhirDB]
level=DEBUG
handlers=fhirDBHandler
propagate=1
qualname=utils.database.fhir_database

[logger_allscriptsServer]
level=DEBUG
handlers=allscriptsServerHandler
propagate=1
qualname=clientApp.module_webservice.allscripts_server

[handler_fhirDBHandler]
class=logging.handlers.TimedRotatingFileHandler
level=DEBUG
formatter=verboseFormatter
args=('/tmp/fhir_db.log','midnight')

[handler_allscriptsServerHandler]
class=logging.handlers.TimedRotatingFileHandler
level=DEBUG
formatter=verboseFormatter
args=('/tmp/allscripts_server.log','midnight')

[handler_tmpHandler]
class=logging.handlers.TimedRotatingFileHandler
level=DEBUG
formatter=verboseFormatter
args=('/tmp/maven.log','midnight')

[handler_compEvalHandler]
class=logging.handlers.TimedRotatingFileHandler
level=DEBUG
formatter=verboseFormatter
args=('/tmp/comp_eval.log','midnight')

[formatter_verboseFormatter]
format=%(asctime)s     %(levelname)s	%(process)d	%(filename)s:%(lineno)d	%(message)s
datefmt=